          autoplay: true,
          autoplayTimeout: 5000,
          autoplayHoverPause: true,
          nav: false,
          dots: false
        });


        $('.nonloop').owlCarousel({
          center: true,
          items: 1,
          loop: true,
          margin: 10,
          autoplay: true,
          autoplayTimeout: 5000,
          autoplayHoverPause: true,
          nav: false,
          dots: false
        });
      });



