<div class="sign-in p-4">
  <div class="d-flex align-items-start justify-content-between mb-4">
      <div>
          <span class="mdi mdi-account-circle-outline display-1 text-primary"></span>
          <h2 class="my-3 fw-bold">Let's Forgot Password</h2>
          <p class="text-muted mb-0"></p>
      </div>

  </div>


  
  <form method="POST" action="/email" class="auth-form">
      <div class="form-group">
          <label for="inputusername" class="form-label">Email Address</label>
          <div class="form-input mb-4">
              <input type="email" class="form-control" value="" id="inputusername" name="value" placeholder="Enter Email Address">
              <input type="hidden" name="type" value="email">
              <i class="ri-user-line user"></i>
          </div>
                  </div>


      <div class="submit-btn">
          <button type="submit" class="btn theme-btn">Reset Password</button>
      </div>
  </form>

</div>





